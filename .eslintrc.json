{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "env": {
        "node": true
    },
    "rules": {
        "prettier/prettier": "off",
        "indent": ["error", 4],
        "no-underscore-dangle": "off",
        "linebreak-style": "off",
        "no-console": "off"
    }
}
