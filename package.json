{
    "name": "zeroslope",
    "description": "Zeroslope API written in Node.",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "assert": "^1.4.1",
        "body-parser": "^1.18.3",
        "colors": "^1.3.3",
        "connect-mongo": "^2.0.1",
        "consul": "^0.34.1",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^6.2.0",
        "errorhandler": "^1.5.0",
        "express": "^4.16.3",
        "express-jwt": "^5.3.1",
        "express-session": "^1.15.6",
        "fs": "0.0.1-security",
        "fs-extra": "^7.0.1",
        "inversify": "^5.0.1",
        "jsonwebtoken": "^8.4.0",
        "mongoose": "^5.4.4",
        "morgan": "^1.9.1",
        "swagger-jsdoc": "3.2.4"
    },
    "main": "server.js",
    "scripts": {
        "prestart": "npm i",
        "start:prod": "npm run build && NODE_ENV=production node dist/server.js",
        "start:dev": "npm run build && node server.js",
        "start": "nodemon --ignore 'swagger/swagger.json' --exec babel-node -- server.js",
        "eslint": "eslint .",
        "clean": "rm -rf dist && mkdir dist",
        "build": "npm run clean && npm run eslint && babel . --out-dir dist --ignore node_modules,.git,.vscode,.idea,.env,test,*/swagger-ui.js,*/swagger-ui-bundle.js",
        "build:watch": "npm run clean && npm run eslint && babel . --out-dir dist --watch --source-maps --ignore node_modules,.git,.vscode,.idea,.env,test,*/swagger-ui.js,*/swagger-ui-bundle.js",
        "test:watch": "mocha test/**/*test.js -w --require @babel/register",
        "test": "mocha test/**/*test.js --require @babel/register",
        "install": "",
        "postinstall": ""
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/node": "^7.2.2",
        "@babel/preset-env": "^7.2.3",
        "@babel/register": "^7.0.0",
        "eslint": "^5.12.1",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^4.0.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.0",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "events": "^3.0.0",
        "mocha": "^5.2.0",
        "node-mocks-http": "^1.7.3",
        "nodemon": "^1.18.9",
        "open": "0.0.5",
        "pg": "^7.8.0",
        "prettier": "^1.16.1",
        "should": "^13.2.3",
        "winston": "^3.1.0"
    },
    "engines": {
        "node": "8.11.1",
        "npm": "5.6.0"
    }
}
